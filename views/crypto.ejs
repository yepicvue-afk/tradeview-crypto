<div>
    <h2>Données Bitcoin - 24 dernières heures</h2>
    <table border="1">
        <tr>
            <th>Date</th>
            <th>Open</th>
            <th>High</th>
            <th>Low</th>
            <th>Close</th>
            <th>Volume</th>
        </tr>
        <% if (data && data.length > 0) { %>
            <% data.forEach(function(row) { %>
                <tr>
                    <td><%= new Date(row.timestamp).toLocaleString() %></td>
                    <td><%= row.open %></td>
                    <td><%= row.high %></td>
                    <td><%= row.low %></td>
                    <td><%= row.close %></td>
                    <td><%= row.volume %></td>
                </tr>
            <% }); %>
        <% } else { %>
            <tr>
                <td colspan="6">Aucune donnée disponible</td>
            </tr>
        <% } %>
    </table>
</div>